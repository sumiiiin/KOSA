package ch07.sec04.exam02;

public class SupersonicAirplaneExample {

	public static void main(String[] args) {
		
		SupersonicAirplane spa = new SupersonicAirplane();
		
		spa.takeOff(); // 이륙을 함
		
		spa.fly(); // 비행을 함 
		
		spa.flyMode = SupersonicAirplane.SUPERSONIC; // 모드 설정
		
		spa.fly();
		
		spa.land();

	}

}
