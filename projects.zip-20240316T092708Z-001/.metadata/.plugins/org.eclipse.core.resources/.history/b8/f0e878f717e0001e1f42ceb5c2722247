package ch08.sec05;

public class RemoteControlExample {

	public static void main(String[] args) {
		
		// 인터페이스 변수 선언
		RemoteControl rc = null; // 널로 초기화
		
		// 인터페이스로 사용할 객체 연결
//		rc = new Television(); // 텔레비전 객체로 초기화
		
		rc.turnOn();
		rc.setVolume(5);
		rc.turnOff();
		System.out.println();

		
	}
	

}
