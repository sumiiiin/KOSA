package ch07.sec10.exam02;

public abstract class Animal {
	
	//field
	String color;
	String kind;
	
	// Constructor
	public Animal(String kind, String color) {
		this.color = color;
		this.kind = kind;
	}
	
	//method
	public void breathe() {
		System.out.println("숨을 쉽니다.");
	}
	
	public abstract void sound();
}
